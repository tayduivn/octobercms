1567136307a:3:{i:0;O:8:"stdClass":23:{s:2:"id";i:160;s:7:"user_id";i:1;s:5:"title";s:66:"Không lo mất dữ liệu với "Thùng rác" của CloudPro CRM";s:4:"slug";s:51:"khong-lo-mat-du-lieu-voi-thung-rac-cua-cloudpro-crm";s:7:"excerpt";s:0:"";s:7:"content";s:2247:"<p>
